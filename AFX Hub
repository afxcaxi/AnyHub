pawn(function() 
    repeat
        task.wait()
    until game:IsLoaded()
    repeat
        task.wait()
    until game.Players
    repeat
        task.wait()
    until game.Players.LocalPlayer and game.Players.LocalPlayer.Team ~= nil 
    wait(1.5)
    require(game.ReplicatedStorage.Notification).new("<Color=Red>Chào Mừng Đến Với AFX Hub<Color=/>"):Display()
    require(game.ReplicatedStorage.Notification).new("<Color=Blue>Discord : Tham Gia Discord Của AfxcaxiRoblox Ðể Có Thông Tin Mới Nhất Nhé <Color=/>"):Display()
    require(game.ReplicatedStorage.Notification).new("<Color=Yellow>Cảm Ơn Vì Đã Sử Dụng Script Của Afxcaxi<Color=/>"):Display()
end)
loadstring(game:HttpGet("https://raw.githubusercontent.com/afxcaxi/AnyHub/main/AfxcaxiHub"))()
